# Deep Tree Echo - Environment Configuration
# Copy this file to .env and configure your settings

# WebSocket connection URL for Deep Tree Echo backend
ECHO_WS_URL=ws://localhost:5000/ws

# API endpoint URL for Deep Tree Echo backend
ECHO_API_URL=http://localhost:5000

# Port for the Node.js client application
PORT=3000

# Optional: Echo Home directory for Aphrodite Engine
ECHO_HOME=/opt/echocog

# Optional: Aphrodite Engine port
APHRODITE_PORT=8080

# Optional: vLLM Attention Backend
VLLM_ATTENTION_BACKEND=FLASHINFER
